{
  "name": "nestjs-douyin-api",
  "version": "0.1.1",
  "description": "基于 douyin-api 封装的抖音生态服务端接口",
  "main": "dist/index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint 'lib/**/*.ts' --fix"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.13",
    "douyin-api": "^0.1.0"
  },
  "devDependencies": {
    "@nestjs/common": "^9.1.2",
    "@nestjs/core": "^9.1.2",
    "@types/node": "^18.7.23",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "files": ["dist"],
  "engines": {
    "node": ">=16"
  },
  "license": "MIT",
  "homepage": "https://github.com/wytxer/nestjs-douyin-api/#readme",
  "keywords": [
    "nest",
    "nestjs",
    "toutiao",
    "douyin",
    "bytedance",
    "microapp",
    "miniapp",
    "抖音",
    "抖音小程序",
    "头条小程序"
  ],
  "author": {
    "name": "wytxer",
    "url": "https://github.com/wytxer"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wytxer/nestjs-douyin-api.git"
  },
  "bugs": {
    "url": "https://github.com/wytxer/nestjs-douyin-api/issues"
  }
}
